import sys

from swift.common.wsgi import run_wsgi

if __name__ == '__main__':
    if len(sys.argv) != 2:
        sys.exit("Usage: %s CONFIG_FILE" % sys.argv[0])
    run_wsgi(sys.argv[1], 'proxy-server', default_port=8080)
